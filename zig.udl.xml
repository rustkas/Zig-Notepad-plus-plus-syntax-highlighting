<NotepadPlus>
    <AutoComplete>
        <KeyWords name="Zig">
            <KeyWord name="fn" />
            <KeyWord name="pub" />
            <KeyWord name="struct" />
            <KeyWord name="const" />
            <KeyWord name="var" />
            <KeyWord name="if" />
            <KeyWord name="else" />
            <KeyWord name="for" />
            <KeyWord name="while" />
            <KeyWord name="break" />
            <KeyWord name="continue" />
            <KeyWord name="return" />
            <KeyWord name="true" />
            <KeyWord name="false" />
            <KeyWord name="null" />
            <KeyWord name="import" />
            <KeyWord name="export" />
            <KeyWord name="using" />
            <KeyWord name="as" />
            <KeyWord name="extern" />
            <KeyWord name="inline" />
            <KeyWord name="defer" />
            <KeyWord name="comptime" />
